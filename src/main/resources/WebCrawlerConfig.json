{
  "maxDepth": 2,
  "includeUrlPatterns": [],
  "excludeUrlPatterns": [],
  "userAgent": "SmileyfaceWebCrawler/0.1",
  "requestTimeoutMs": 10000,
  "queueNamespace": "crawler",
  "indexPrefix": "crawler-main",

  "pages": [
    {
      "urlPattern": "^https://www.666forum.com.*",
      "matchAll": true,
      "contentRules": {
        "minCharacter": 600,
        "tagName": "span",
        "elementStyle": "font-size: 24px; line-height: normal"
      }
    },
    {
      "urlPattern": "^https://science.nasa.gov.*",
      "contentRules": {
        "minCharacter": 600
      }
    }
  ]
}